{
    "manifest_version": 3,
    "name": "Twitter Revert Rebrand",
    "description": "Chrome extension to revert the Twitter UI to the pre-Musk era",
    "homepage_url": "https://github.com/exieneko/twitter-revert-rebrand",
    "version": "1.3.0",
    "icons": {
        "128": "images/twitter-128.png"
    },
    "content_scripts": [{
        "js": ["content.js"],
        "matches": ["https://twitter.com/*", "https://x.com/*"]
    }],
    "web_accessible_resources": [{
        "resources": ["images/*"],
        "matches": ["https://twitter.com/*", "https://x.com/*"]
    }]
}
